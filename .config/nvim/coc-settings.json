{
  "diagnostic.checkCurrentLine": true,
  "coc.preferences.formatOnSaveFiletypes": ["*"],
  "coc.preferences.extensionUpdateCheck": "daily",
  "suggest.noselect": false,
  "eslint.autoFixOnSave": true,
  "eslint.probe": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "html",
    "svelte",
    "vue",
    "markdown"
  ],
  "svelte.enable-ts-plugin": true
}
